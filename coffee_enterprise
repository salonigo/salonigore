   "source": [
    "top_avg_aroma = top_beans['Aroma'].mean()\n",
    "top_aroma_rows = top_beans[top_beans['Aroma'] >= top_avg_aroma]\n",
    "top_aroma_rows = top_aroma_rows.sort_values(by='Aroma', ascending=False)\n",
    "top_aroma_rows"
   ]
